<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
<!--link-->

<link name="robot_footprint"></link>

<link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

 <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
 </inertial>

 <collision name='chassis_collision'>
      <origin xyz="0 0 -0.02" rpy=" 0 0 0"/> 
      <geometry>
        <cylinder length="0.1" radius="0.25"/>
      </geometry>
 </collision>

 <visual name='chassis_visual'>
      <origin xyz="0 0 -0.02" rpy=" 0 0 0"/>
      <geometry>
        <cylinder length="0.1" radius="0.25"/>
      </geometry>
 </visual>

 <collision name='back_caster_collision'>
      <origin xyz="-0.21 0 -0.077" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.022"/>
      </geometry>
 </collision>

 <visual name='back_caster_visual'>
      <origin xyz="-0.21 0 -0.077" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.022"/>
      </geometry>
 </visual>

 <collision name='front_caster_collision'>
      <origin xyz="0.21 0 -0.077" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.022"/>
      </geometry>
 </collision>

 <visual name='front_caster_visual'>
      <origin xyz="0.21 0 -0.077" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.022"/>
      </geometry>
 </visual>

</link>

<link name='upper_chassis'>
    <pose>0 0 0.1 0 0 0</pose>

 <inertial>
      	<mass value="2.0"/>
      	<origin xyz="0.0 0 0" rpy=" 0 0 0"/>
  <inertia
	ixx="0.1" ixy="0" ixz="0"
	iyy="0.1" iyz="0"
  	izz="0.1"
      	/>
 </inertial>

 <collision name='upper_chasis_collision'>
    <origin xyz="0 0 0" rpy=" 0 0 0"/> 
     	<geometry>
    <box size="0.25 .25 .1"/>
    </geometry>
 </collision>

 <visual>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.25 .25 .1"/>
      </geometry>
 </visual>
</link>

<link name='right_wheel'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy="0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision_right_wheel'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </visual>
</link>

<link name='left_wheel'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision_left_wheel'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.05" radius="0.1"/>
      </geometry>
    </visual>

</link>

<link name='camera'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.05 0.05 0.05" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='collision_camera'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>
</link>

<link name='hokuyo'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='collision_hokuyo'>
      <origin xyz="0 0 0" rpy="0 0 0"/> 
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual name='visual_hokuyo'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename = "package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
</link>


<link name="camera_link_optical">
</link>

<!--joint-->

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <joint name="chassis_joint" type="fixed">
    <origin xyz="0 0 0.05" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="upper_chassis" />
  </joint>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0 0.272 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0 -0.272 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.25 0 -0.02" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0 0 .13" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <joint name="camera_optical_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <parent link="camera"/>
    <child link="camera_link_optical"/>
  </joint>
</robot>
